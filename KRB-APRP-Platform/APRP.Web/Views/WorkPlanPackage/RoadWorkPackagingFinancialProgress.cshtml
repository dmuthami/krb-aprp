<script type="text/javascript">

    var SaveWorkplanPackageProgress = function () {
        if ($('#BankReconFile').length) { //check if element exists
            var fileName = $('#BankReconFile').val().replace('C:\\fakepath\\', '');
            var fileNameExtension = "." + fileName.split(".").pop();
            var fileNameBase = fileName.substring(0, fileName.length - fileNameExtension.length);

            if (fileNameBase.length <= 150) { //limit file name size to 100 characters
                var form = $('#contractProgressForm')[0];
                var formData = new FormData(form);
                event.preventDefault();
                $.ajax(
                    {
                        type: "POST",
                        url: "/WorkPlanPackage/SaveWorkplanPackageProgress",
                        data: formData,
                        processData: false,
                        contentType: false,
                        success: function (data) {
                            if (data.success) {
                                //$("#modalViewApprovalId").modal('close');
                                swal({
                                    title: "Success",
                                    text: "Record has been submitted successfully!",
                                    timer: 2500,
                                    icon: "success",
                                    buttons: false
                                }).then(function () {
                                    window.location.href = data.href;
                                });
                            }
                            else {
                                swal(data.message, {
                                    title: 'Error Occured',
                                    icon: "error"
                                });
                            }
                        }
                    }
                );
            }
            else {
                swal({
                    title: 'Warning',
                    icon: 'warning',
                    text: "The filename length has exceeded 150 characters. Please check!"
                })
            }
        }
        else {
            swal({
                title: 'Warning',
                icon: 'warning',
                text: "Please upload supporting reconciliation documents!"
            })
        }
    }

    //delete the record
    var ConfirmDelete = function (financialProgressID) {
        swal({
            title: "Are you sure?",
            text: "The entries for the selected month will be removed!",
            icon: 'warning',
            dangerMode: true,
            buttons: {
                cancel: 'No, Please!',
                delete: 'Yes, Delete It'
            }
        }).then(function (willDelete) {
            if (willDelete) {
                DeleteRecord(financialProgressID);
            }
            else {
                swal("Record has not removed", {
                    title: 'Cancelled',
                    icon: "error",
                });
            }
        });
    }

    var DeleteRecord = function (financialProgressID) {
        $.ajax(
            {
                type: "POST",
                url: "/WorkPlanPackage/DeleteWorkplanPackageProgress",
                data: { financialProgressID: financialProgressID },
                success: function (data) {
                    console.log(data);
                    if (data.success) {
                        window.location.href = data.href;
                    }
                    else {
                        swal(data.message, {
                            title: 'Cancelled',
                            icon: "error",
                        });
                    }
                }
            });
    }

    var DownloadProgress = function () {
        var authorityId = @ViewBag.Authority.ID;
        var fYearId = 0;


        $.ajax({
            cache: false,
            url: '/WorkPlanPackage/DownloadQuarterlyProgressSummary?authorityId=' + authorityId + "&financialYearId=" + fYearId,
            success: function (data) {
                if (data.success) {
                    window.location.href = data.href;
                }
                else {
                    swal(data.message, {
                        title: 'Cancelled',
                        icon: 'error',
                    });
                }
            }
        });

    }



</script>
