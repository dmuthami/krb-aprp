<!-- Content Area Starts -->
<div class="chat-application">
    <div class="chat-content-head">
        <div class="header-details">
            <h5 class="m-0 sidebar-title">Financial ProgressRreporting</h5>
        </div>
    </div>

    <!-- Scroll - vertical, dynamic height -->
    <div class="row">
        <div class="col s12 m12 l12">
            <div class="card">
                <div class="card-content pb-1">
                    <div class="row">
                        <div class="col s12 m12 l12">
                            <a class="waves-effect waves-light btn-small" asp-controller="WorkPlanPackage" asp-action="ProjectProgressDetails" asp-route-id="0"><i class="material-icons left">arrow_back</i> Back to Progress Summary</a>

                        </div>
                    </div>
                </div>
            </div>
            <div class="card">
                <div class="card-content">
                    <div class="row" id="product-four">
                        <div class="col m10 offset-s4">
                            <h6 class="strong">@Model.FinancialProgress.Authority.Name.ToUpper() ANNUAL ROAD WORKS PROGRAMME FINANCIAL REPORT </h6>
                            <div class="row">
                                <div class="col s3"><p class="black-text strong">AGENCY CODE: </p></div>
                                <div class="col s3"><span class="green-text strong">@Model.FinancialProgress.Authority.Code.ToUpper()</span></div>
                            </div>
                            <div class="row">
                                <div class="col s3"><p class="black-text strong">FINANCIAL YEAR :  </p></div>
                                <div class="col s3"><span class="green-text strong">@Model.FinancialProgress.FinancialYear.Code.ToUpper()</span></div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
