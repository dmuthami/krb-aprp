        function DisplayElements(_Code) {
            if (_Code != "KRB") {
                //ARICS Sum Paved
                $("#divAuthoritiesSumPaved").hide();
                $("#divReportingYear").removeClass("m6");

                //ARICS Sum Unpaved
                $("#divAuthoritiesSumUnPaved").hide();
                $("#divReportingYearUnPaved").removeClass("m6");

                //ARICS Master Sum
                $("#divAuthoritiesMasterSum").hide();
                $("#divReportingYearMasterSum").removeClass("m6");

                //ARICS Summary
                $("#divAuthoritiesSummary").hide();
                $("#divReportingYearSummary").removeClass("m6");

                //ARICS Structures
                $("#divAuthoritiesStructure").hide();
                $("#divReportingYearStructure").removeClass("m6");
            }
        }

        function LoadData(_status,AuthorityCode) {

            if (AuthorityCode == "KRB") {
                //if(1)==55 show CG drop down
                if (_status == "55") {
                    //Show CG drop down
                    $('#IdCG').show();
                } else {
                    //hide non-select div
                    $('#IdCG').hide();
                }
            } else {
                //hide all
                $('#IdInstitutionGroup').hide();
                $('#IdCG').hide();
            }

        }

        var ExportPrioritized = function () {
            var Year = $('#ARICSYear_ID option:selected').val()
            var AuthorityCode = $('#Authority_Code').val();
            var AuthorityId = 0;
            if (AuthorityCode == "KRB") {
                //Do something
                var IdInstitutionGroup = $('#IdInstitutionGroup option:selected').val();
                if (IdInstitutionGroup == 55) {
                    AuthorityId = $('#IdCG option:selected').val();
                } else {
                    AuthorityId = IdInstitutionGroup;
                }

                $.ajax(
                    {
                        type: "POST",
                        url: "/Workplan/ExportPrioritizedKRB",
                        data: { Year: Year, AuthorityId: AuthorityId },
                        success: function (data) {
                            console.log(data.value);
                            if (data.value.success) {
                                window.location.href = data.value.href;
                            }
                            else {
                                swal(data.value.message, {
                                    title: 'Cancelled',
                                    icon: "error",
                                });
                            }
                        }
                    }
                );
            } else {
                AuthorityId = $('#Authority_ID').val();

                $.ajax(
