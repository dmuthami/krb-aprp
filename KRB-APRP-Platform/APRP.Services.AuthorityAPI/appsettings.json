{
  "ConnectionStrings": {
    "WebApiDatabase": "User ID=postgres;Password=postgres;Server=localhost;Port=5432;Database=aprp_dev;Pooling=true;"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "File",
        "Args": {
          "path": "./logs/log-.txt",
          "rollingInterval": "Day"
        }
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5341/"
        }
      }
      ]
    
  },
  "AllowedHosts": "*"
}
